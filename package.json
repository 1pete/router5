{
  "name": "router5",
  "version": "0.1.0-rc.6",
  "description": "An HTML5 router, based on route-node and path-parser",
  "main": "dist/commonjs/index.js",
  "scripts": {
    "build": "node ./scripts/build.js && npm run uglify",
    "uglify": "./node_modules/.bin/uglifyjs2 dist/browser/router5.js --comments -m -o dist/browser/router5.min.js",
    "test": "./node_modules/.bin/karma start karma.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/router/router5.git"
  },
  "keywords": [
    "router",
    "html5"
  ],
  "author": "Thomas Roch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/router/router5/issues"
  },
  "homepage": "http://router5.github.io",
  "dependencies": {
    "route-node": "~0.1.1"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "async": "^1.3.0",
    "karma-firefox-launcher": "^0.1.6",
    "jasmine-core": "^2.3.4",
    "karma-jasmine": "^0.3.6",
    "karma-chrome-launcher": "^0.2.0",
    "karma": "^0.12.37",
    "karma-mocha-reporter": "^1.0.2",
    "karma-coverage": "^0.4.2",
    "karma-coveralls": "^1.1.0",
    "coveralls": "^2.11.2",
    "mkdirp": "^0.5.1",
    "uglify-js2": "^2.1.11"
  }
