{
  "name": "react-router5",
  "version": "2.0.0-rc.2",
  "description": "router5 helpers for React",
  "main": "dist/commonjs/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "babel modules --out-dir dist/commonjs",
    "build:umd": "rollup -c rollup.config.js -f umd",
    "build:amd": "rollup -c rollup.config.js -f amd",
    "build": "npm run clean && npm run build:cjs && npm run build:amd && npm run build:umd",
    "test": "mocha --compilers js:babel-core/register --recursive --require ./test/test-helper.js 'test/main.js'",
    "clog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "lint": "eslint modules",
    "release": "./scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/router5/react-router5.git"
  },
  "keywords": [
    "router",
    "html5",
    "history",
    "tree",
    "react",
    "functional"
  ],
  "author": "Thomas Roch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/router5/react-router5/issues"
  },
  "homepage": "http://router5.github.com/router5/react-router5",
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-export-extensions": "^6.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "chai-enzyme": "^0.2.1",
    "cheerio": "^0.19.0",
    "enzyme": "^1.3.1",
    "eslint": "^1.10.3",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.6",
    "react-dom": "^0.14.6",
    "rimraf": "^2.5.0",
    "rollup": "^0.24.1",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-npm": "^1.2.1",
    "router5": "2.0.0-rc.1",
    "router5-listeners": "2.0.0-rc.1",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "yargs": "^3.27.0"
  },
  "peerDependencies": {
    "router5": "2.0.0-rc.1"
  },
  "dependencies": {
    "react": "^0.14.0"
  }
}
