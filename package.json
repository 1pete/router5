{
  "name": "router5",
  "version": "1.3.0",
  "description": "An HTML5 router, based on route-node and path-parser",
  "main": "dist/commonjs/index.js",
  "jsnext:main": "modules/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "babel modules --out-dir dist/commonjs",
    "build:umd": "rollup -c rollup.config.js --format umd",
    "build:amd": "rollup -c rollup.config.js --format amd",
    "build:iife": "rollup -c rollup.config.js --format iife",
    "build": "npm run clean && npm run build:cjs && npm run build:amd && npm run build:umd && npm run build:iife",
    "test:client": "mocha --compilers js:babel-core/register --recursive --require ./tests/_helpers.js 'tests/client.js'",
    "test:server": "mocha --compilers js:babel-core/register --recursive 'tests/server.js'",
    "test": "npm run test:client && npm run test:server",
    "clog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "lint": "eslint modules",
    "release": "./scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/router5/router5.git"
  },
  "keywords": [
    "router",
    "routing",
    "html5",
    "functional"
  ],
  "author": "Thomas Roch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/router/router5/issues"
  },
  "homepage": "http://router5.github.io",
  "dependencies": {
    "route-node": "~1.0.2",
    "router5.transition-path": "~2.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^3.1.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-export-extensions": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.0.0",
    "chai": "^3.4.1",
    "conventional-changelog": "^0.5.3",
    "del": "^2.0.2",
    "eslint": "^0.24.1",
    "jsdom": "^7.2.2",
    "mocha": "^2.2.5",
    "rimraf": "^2.5.0",
    "rollup": "^0.24.0",
    "rollup-plugin-babel": "^2.3.6",
    "rollup-plugin-npm": "^1.2.0",
    "rollup-plugin-uglify": "^0.1.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "yargs": "^3.15.0"
  }
}
